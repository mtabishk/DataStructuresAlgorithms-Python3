   "source": [
    "def maxArrayIndex(n, arr):\n",
    "    j = n-1\n",
    "    k = -1\n",
    "    for i in range(0,n-1):\n",
    "        if (arr[i] < arr[j]):\n",
    "            k = i\n",
    "            return j-k\n",
    "    return -1"
   ]
